function createRatingButtons(){var e=Titanium.UI.createButtonBar({labels:[{title:"Very Bad",image:"img/unlike.png"},{title:"So-So",image:"img/so_so.png"},{title:"Loved it!",image:"img/like.png"}],backgroundColor:"blue",top:141,height:40,width:"100%"});scrollViewDetailsWindow.add(e),e.addEventListener("click",function(t){0==t.index?(sendRating(placeID,userID,2),detailsWindowButtonSet.show()):1==t.index?(sendRating(placeID,userID,5),detailsWindowButtonSet.show()):2==t.index&&(sendRating(placeID,userID,9),detailsWindowButtonSet.show()),scrollViewDetailsWindow.remove(e)})}