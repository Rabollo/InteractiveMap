function createDetailsWindowObjects(e){var t=e;placeID=e;var i="http://dragomen.com/interactivemap/sortdatadetails.php?id="+t;xhr.onload=function(){var e=JSON.parse(this.responseText),i=[];i[0]="KS_nav_ui.png",i[1]="KS_nav_ui.png",i[2]="KS_nav_ui.png";for(var o=0;o<e.length;o++)i[o]=e[o].p_link;var a="white",n="white",r=Titanium.UI.createImageView({backgroundColor:a,height:100,width:105,top:0,left:0,borderColor:n,image:i[0]}),d=Titanium.UI.createImageView({backgroundColor:a,height:100,width:110,top:0,left:105,borderColor:n,image:i[1]}),l=Titanium.UI.createImageView({backgroundColor:a,height:100,width:105,top:0,left:215,borderColor:n,image:i[2]});scrollViewDetailsWindow.add(r),scrollViewDetailsWindow.add(d),scrollViewDetailsWindow.add(l),detailsWindowButtonSet=Titanium.UI.createButtonBar({labels:[{title:"Rate"},{title:"Leave a Tip"},{title:"Save"}],backgroundColor:"blue",top:100,height:40,width:"100%"}),scrollViewDetailsWindow.add(detailsWindowButtonSet),detailsWindowButtonSet.addEventListener("click",function(e){if(0==e.index)createRatingButtons();else if(1==e.index){var i=Ti.UI.createButton({systemButton:Ti.UI.iPhone.SystemButton.CANCEL});leaveATip.setLeftNavButton(i),i.addEventListener("click",function(){navWin.close()});var o=Ti.UI.createButton({title:"Post",marginRight:"10"});leaveATip.setRightNavButton(o),o.addEventListener("click",function(){""==n.value?alert("You con not send empty comment."):(sendComment(placeID,userID,n.value),navWin.close(),createDetailsWindowLoadComments(t))});var a=Titanium.UI.createLabel({font:{fontSize:18},color:"blue",text:"What's good here?",height:"10%",width:"100%",top:"0",left:"5"});leaveATip.add(a);var n=Titanium.UI.createTextArea({font:{fontSize:24},height:"30%",width:"100%",top:"9%"});leaveATip.add(n),n.addEventListener("click",function(){n.focus()}),navWin.addEventListener("open",function(){n.focus()}),navWin.open({modalTransitionStyle:Ti.UI.iPhone.MODAL_TRANSITION_STYLE_FLIP_HORIZONTAL,modalStyle:Ti.UI.iPhone.MODAL_PRESENTATION_PAGESHEET})}else 2==e.index&&saveplace(placeID,userID)});var c=MapModule.createView({height:130,top:140,mapType:MapModule.NORMAL_TYPE,userLocation:!0,visible:!0,latitudeDelta:.05,longitudeDelta:.05});scrollViewDetailsWindow.add(c),annotationL[e[0].p_id]=MapModule.createAnnotation({latitude:e[0].p_lat,longitude:e[0].p_long,title:e[0].p_name,subtitle:e[0].p_description,rightButton:Titanium.UI.iPhone.SystemButton.INFO_LIGHT,myid:e[0].p_id}),c.setLocation({latitude:e[0].p_lat,longitude:e[0].p_long,animate:!0,latitudeDelta:.01,longitudeDelta:.01}),c.addAnnotation(annotationL[e[0].p_id]),createDetailsWindowLoadComments(t)},xhr.onerror=function(e){alert("Network error "+e.error)},xhr.open("GET",i),xhr.setRequestHeader("content-type","application/json"),xhr.send()}var annotationL=[],detailsWindowButtonSet;