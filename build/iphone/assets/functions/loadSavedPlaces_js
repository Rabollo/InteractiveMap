function loadSavedPlacesList(e){IDArray2.length=0;var t=e,i="http://dragomen.com/interactivemap/loadsavedplaces.php?userid="+t,o=[];xhr.onload=function(){for(var e=JSON.parse(this.responseText),t=0;t<e.length;t++){var i=Ti.UI.createTableViewRow({selectedBackgroundColor:"White",height:80});savedWindowTable.title=e[t].p_id;var a=Ti.UI.createLabel({width:"100%",height:"100%",backgroundColor:"white"}),n=Ti.UI.createImageView({image:e[t].p_link,top:5,left:"5%",height:"70",width:"70",borderRadius:3}),r=Ti.UI.createLabel({text:e[t].p_name,font:{fontSize:14,fontWeight:"bold"},left:"30%",top:"10%",color:"blue"}),d=Ti.UI.createLabel({text:e[t].p_description,font:{fontSize:12},left:"30%",top:"43%",color:"Gray",width:"55%",height:"40%"}),l=Ti.UI.createLabel({text:" "+e[t].AvarageOfRatings+" ",font:{fontSize:14},right:"5%",top:"40%",bottom:"10%",width:"auto",height:"auto",color:"white",backgroundColor:"#FF6600",borderRadius:5});i.add(a),i.add(r),i.add(d),i.add(l),i.add(n),o.push(i),IDArray2.push(e[t].p_id)}savedWindowTable.data=o},xhr.onerror=function(e){alert("Network error "+e.error)},xhr.open("GET",i),xhr.setRequestHeader("content-type","application/json"),xhr.send()}